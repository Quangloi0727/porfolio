    <% include header %>
        <!-- lead -->
        <% duLieuJson.dulieu.forEach(function(spchitiet){ %>
        <%
            if (spchitiet.id==idsp){ %>
            <div class="ish-row ish-fullrow ish-lead">
            <span class="ish-lead__caption ish-icon-up"><%= spchitiet.title %></span>
            <div class="ish-lead__image">
                <img src="<%= spchitiet.bigImage %>" alt="wawererly design">
            </div>
            <h1 class="ish-lead__headline">
				<span class="ish-lead__headline-inner"><%= spchitiet.title %><br><%= spchitiet.cat %></span>
            </h1>
        </div>
      
 
        <!-- lead end -->



        <!-- content -->
        <div class="ish-row ish-content">

            <div class="ish-col-xs-12 ish-col-sm-4 ish-last-xs ish-first-sm ish-content__sidebar">

                <!-- side content -->
                <p> <%= spchitiet.des %> </p>

                <ul class="ish-sidebar-mdata post-meta">
                    <li class="ish-sidebar-mdata__item">
                        <strong class="ish-sidebar-mdata__title post-meta-key">Khách hàng</strong>
                        <%= spchitiet.cilent %>
                    </li>
                    <li class="ish-sidebar-mdata__item">
                        <strong class="ish-sidebar-mdata__title post-meta-key">Vai trò</strong>
                        <%= spchitiet.role %>
                    </li>
                    <li class="ish-sidebar-mdata__item">
                        <strong class="ish-sidebar-mdata__title post-meta-key">Danh mục</strong>
                        <a href="#"> <%= spchitiet.cat %> </a>
                    </li>
                    <li class="ish-sidebar-mdata__item">
                        <strong class="ish-sidebar-mdata__title post-meta-key">Link</strong>
                        <a href="<%= spchitiet.link %>" target="_blank"><%= spchitiet.link %></a>
                    </li>
                </ul>

                <hr>

                <div class="ish-sidebar-share">
                    <strong class="ish-sidebar-share__desc">Share</strong>
                    <ul class="ish-sidebar-share__list">
                        <li class="ish-sidebar-share__list-item">
                            <a href="#" class="ish-sidebar-share__list-link ish-sc-el ish-sc-icon ish-icon-facebook ish--tc3"></a>
                        </li>
                        <li class="ish-sidebar-share__list-item">
                            <a href="#" class="ish-sidebar-share__list-link ish-sc-el ish-sc-icon ish-icon-behance ish--tc3"></a>
                        </li>
                        <li class="ish-sidebar-share__list-item">
                            <a href="#" class="ish-sidebar-share__list-link ish-sc-el ish-sc-icon ish-icon-dribbble ish--tc3"></a>
                        </li>
                        <li class="ish-sidebar-share__list-item">
                            <a href="#" class="ish-sidebar-share__list-link ish-sc-el ish-sc-icon ish-icon-twitter ish--tc3"></a>
                        </li>
                    </ul>
                </div>

                <hr>

                <ul class="ish-prev-next">
                    <li class="ish-prev-next__item ish-prev-next--left">
                        <a class="ish-prev-next__link" href="portfolio-detail2.html">Previous</a>
                        <span class="ish-prev-next__icon ish-icon-left"></span>
                    </li>
                    <li class="ish-prev-next__item ish-prev-next--right">
                        <a class="ish-prev-next__link" href="portfolio-detail2.html">Next</a>
                        <span class="ish-prev-next__icon ish-icon-right"></span>
                    </li>
                </ul>
                <!-- side content end -->

            </div>

            <div class="ish-col-xs-12 ish-col-sm-7 ish-col-sm-offset-1 ish-content__main">

                <!-- main content -->
               <% spchitiet.productImage.forEach(function(motanh) { %>
                <a class="ish-image ish-lightbox" href="<%= motanh %>">
                    <img class="ish-image__image" src="<%= motanh %>" alt="logotype design">
                    <span class="ish-image__caption">Logotype Design</span>
                </a>
                <!-- main content end -->
                <%
                }) %>
            </div>

        </div>
            <% }
               }) %>   
        <!-- content end -->

    <% include footer %>